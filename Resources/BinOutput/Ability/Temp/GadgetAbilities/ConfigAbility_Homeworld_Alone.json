                  ]
                },
                {
                  "$type": "ChangeGadgetUIInteractHint",
                  "hintTextMapId": "UI_HOMEWORLD_FARMFIELD_BUTTONB",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_ABILITY_HomeField_Mark",
                      "value": 2,
                      "compareType": "Equal"
                    }
                  ]
                },
                {
                  "$type": "ChangeGadgetUIInteractHint",
                  "hintTextMapId": "UI_HOMEWORLD_FARMFIELD_BUTTONC",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_ABILITY_HomeField_Mark",
                      "value": 3,
                      "compareType": "Equal"
                    }
                  ]
                },
                //根据GadgetID显示不同的种植内容
                {
                  "$type": "EnableGadgetIntee",
                  "enable": true
                }
              ]
            },
            {
              "$type": "EnableSceneTransformByName",
              "transformNames": ["FarmSeeding"],
              "setEnable": false
            }
          ]
        },
        "FarmState1": {
          //土包阶段，显示成熟时间
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "CurLocalAvatar",
              "targetPredicates": [
                {
                  "$type": "ByHostOrGuest",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  "$type": "EnableGadgetIntee",
                  "enable": false
                },
                {
                  "$type": "EnableGadgetIntee",
                  "enable": true
                }
              ]
            },

            {
              "$type": "EnableSceneTransformByName",
              "transformNames": ["FarmSeeding"],
              "setEnable": true
            },

            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Homeworld_Field_01_WaterBubble",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_HomeField_Mark",
                  "value": 3,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "FarmState2": {
          //苗阶段，显示成熟时间
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "CurLocalAvatar",
              "targetPredicates": [
                {
                  "$type": "ByHostOrGuest",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  "$type": "EnableGadgetIntee",
                  "enable": false
                },
                {
                  "$type": "EnableGadgetIntee",
                  "enable": true
                }
              ]
            },
            {
              "$type": "EnableSceneTransformByName",
              "transformNames": ["FarmSeeding"],
              "setEnable": true
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Homeworld_Field_01_WaterBubble",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_HomeField_Mark",
                  "value": 3,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "FarmState3": {
          //采集阶段，显示成熟时间
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "CurLocalAvatar",
              "targetPredicates": [
                {
                  "$type": "ByHostOrGuest",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  "$type": "EnableGadgetIntee",
                  "enable": false
                }
              ]
            },
            {
              "$type": "EnableSceneTransformByName",
              "transformNames": ["FarmSeeding"],
              "setEnable": true
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Homeworld_Field_01_WaterBubble",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_HomeField_Mark",
                  "value": 3,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        }
      }
    }
