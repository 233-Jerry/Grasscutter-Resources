      "abilityName": "Monster_Effigy_Fire_FightingPoseManager",
      "abilityMixins": [
        //Cooldown
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [101],
          "modifierName": "Effigy_Fire_CooldownModifier"
        },
        //Burning
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [201],
          "modifierName": "Effigy_Fire_BurningModifier"
        },
        {
          "$type": "ServerFinishWatcherMixin",
          "watcherId": 82097,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "target": "Self",
              "key": "_WATCHER_82097_",
              "value": 1,
              "compareType": "Equal"
            }
          ]
        },
        {
          "$type": "ServerFinishWatcherMixin",
          "watcherId": 82102,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "target": "Self",
              "key": "_WATCHER_82097_",
              "value": 3,
              "compareType": "MoreThanAndEqual"
            }
          ]
        }
      ],
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_EFFIGY_FIRE_REVIVECOUNT_",
          "value": 0
        }
      ],
      "modifiers": {
        "Effigy_Fire_CooldownModifier": {
          //Cooldown state
          //TODO:
          //End Burning fx on enter
          //End by ignite skill
          //Handles chargeBar
          "thinkInterval": 2.0,
          "modifierMixins": [
            {
              "$type": "ChargeBarMixin",
              "chargeBarElementType": "Fire",
              "chargeBarIconID": 2, //怪物状态专用图标
              "initialValue": 0,
              "maxValue": 100
            },
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_CHARGEBAR_VALUE", //检测重生时剩余柱子
              "valueSteps": [99.5, 100.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": ["IgniteSuccessActions"]
            }
          ],
          "onThinkInterval": [
            {
              "$type": "AddChargeBarValue",
              "value": 0.01
            },
            {
              "$type": "AddChargeBarValue",
              "value": -0.01
            }
          ],
          "onAdded": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "ShaderBurning",
              "value": false,
              "canBeHandledOnRecover": true
            }
          ]
        },
        "IgniteSuccessActions": {
          "onAdded": [
            //Start burning
            {
              "$type": "SetPoseBool",
              "boolID": "Burning",
              "value": true
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "IgniteSuccessTrigger"
            },
            {
              "$type": "ServerMonsterLog",
              "target": "Self",
              "paramList": [2, 1, 1]
            }
          ]
        },
        "Effigy_Fire_BurningModifier": {
          //Burning state
          //TODO:
          //Maintain Burning fx
          //Fire ShieldBar
          //Play extinct anim on end
          //"state": "LockHP",
          "elementType": "Fire",
          "elementDurability": 700,
          "thinkInterval": 5.0,
          "modifierMixins": [
            {
              "$type": "ShieldBarMixin",
              "useMutiPlayerFixData": true,
              "onShieldBroken": [
                //Start burning
                {
                  "$type": "SetPoseBool",
                  "boolID": "Burning",
                  "value": false
                },
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "ExtinctTrigger"
                },
                {
                  "$type": "AddGlobalValue",
                  "key": "_WATCHER_82097_",
                  "value": 1,
                  "useLimitRange": true,
                  "minValue": 0,
                  "maxValue": 10
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "ShaderBurningTrigger",
              "MPTriggerOnRemote": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByTargetHPRatio",
                  "HPRatio": "%Effigy_Fire_ReviveMaxHPRatio",
                  "logic": "LesserOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "AddGlobalValue",
                  "target": "Self",
                  "key": "_EFFIGY_FIRE_REVIVECOUNT_",
                  "value": 1,
                  "useLimitRange": true,
                  "maxValue": 3,
                  "minValue": 0
                }
              ]
            },
            //////////////////////////////////////////////////////
            //Check revive count to set revive math
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_EFFIGY_FIRE_REVIVECOUNT_",
                  "value": 0,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "SetOverrideMapValue",
