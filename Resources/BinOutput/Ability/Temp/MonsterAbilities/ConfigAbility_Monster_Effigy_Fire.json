                {
                  "$type": "SetOverrideMapValue",
                  "overrideMapKey": "Effigy_Fire_ReviveHPStep",
                  "value": 0.025
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_EFFIGY_FIRE_REVIVECOUNT_",
                  "value": 3,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "SetOverrideMapValue",
                  "overrideMapKey": "Effigy_Fire_ReviveHPStep",
                  "value": 0.0
                }
              ]
            },
